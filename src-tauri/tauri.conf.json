{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "SJTU Canvas Helper",
    "version": "1.2.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "process": {
        "relaunch": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true
      }
    },
    "windows": [
      {
        "title": "SJTU Canvas Helper",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://github.com/Okabe-Rintarou-0/SJTU-Canvas-Helper/releases/download/latest/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlBNDQ2MDFFNDc4Qzk5RTEKUldUaG1ZeEhIbUJFbW9BMk85WFQrYzBTclZKNHdsS2xDYWp6M1h0MjRWR0hGWE5LOXgwdW9XWFQK",
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.okabe.sjtu.canvas",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}